{
  "name": "@cmdcode/frost",
  "version": "1.1.0",
  "description": "Flexible, round-optimized schnorr threshold signatures for Bitcoin.",
  "author": "Christopher Scott",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cmdruid/frost"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "src"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./ecc": {
      "import": "./dist/ecc/index.js",
      "types": "./dist/ecc/index.d.ts",
      "default": "./dist/ecc/index.js"
    },
    "./lib": {
      "import": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/index.js"
    },
    "./util": {
      "import": "./dist/util/index.js",
      "types": "./dist/util/index.d.ts",
      "default": "./dist/util/index.js"
    }
  },
  "scripts": {
    "build": "./script/build.sh",
    "load": "tsx --tsconfig ./test/tsconfig.json",
    "release": "npm test && npm run build",
    "scratch": "npm run load test/scratch.ts",
    "test": "npm run load test/tape.ts | faucet"
  },
  "devDependencies": {
    "@cmdcode/core-cmd": "^1.6.5",
    "@cmdcode/crypto-tools": "^2.8.0",
    "@cmdcode/tapscript": "^1.4.6",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^22.9.0",
    "@types/tape": "^5.6.4",
    "faucet": "^0.0.4",
    "rollup": "^4.24.4",
    "tape": "^5.9.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@cmdcode/buff": "^2.2.5",
    "@noble/curves": "^1.6.0",
    "@noble/hashes": "^1.5.0"
  },
  "packageManager": "yarn@4.6.0+sha512.5383cc12567a95f1d668fbe762dfe0075c595b4bfff433be478dbbe24e05251a8e8c3eb992a986667c1d53b6c3a9c85b8398c35a960587fbd9fa3a0915406728"
}
